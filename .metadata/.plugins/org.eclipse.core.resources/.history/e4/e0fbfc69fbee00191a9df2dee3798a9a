package practice;

public class reverse {
	
	private int answer;
	private int lastdigit;

	public void findreverse(int num) {
		// TODO Auto-generated method stub
		
		answer = 0;
		while(num > 0) {
			
			lastdigit = num % 10;
			answer = (answer * 10) + lastdigit;
			num = num / 10;
			
		}
		
	}

}
