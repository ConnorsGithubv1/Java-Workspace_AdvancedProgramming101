package practice;

import javax.swing.JOptionPane;

public class countPairsRunner {

	public static void main(String args[]) {
		
		JOptionPane.showMessageDialog(null, "enter a number to get it in reverse order");
		String word = JOptionPane.showInputDialog("enter number:");
		countPairs number = new countPairs(word);
		
		number.findpairs();
	}
}
